{
  "version": 2,
  "builds": [
    {
      "src": "services/auth-service/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "services/payment-service/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/auth-service/(.*)", "dest": "/api/auth-service/index.js" },
    { "src": "/payment-service/(.*)", "dest": "/api/payment-service/index.js" }
  ]
}
